#include <iostream>
using namespace std;

int main(int argc, char * argv[])
{
    cout << "Hello world!" << endl;
    cout << "The arg num is " << argc << endl 
        << "They are " << endl;
    for(int i = 0; i < argc; ++ i)
    {
        cout << argv[i] << endl;
        argv++;
    }
    cout << "out " << endl;
    return 0;
}